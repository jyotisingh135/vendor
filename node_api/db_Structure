tblAgent :-
agentId:{
    type:Sequelize.INTEGER,
    primaryKey:true,
    autoIncrement:true
},
agentName:{
    type:Sequelize.STRING(40),
    allowNull:false
},
companyName:{
    type:Sequelize.STRING(40),
    allowNull:false
},
street:{
    type:Sequelize.STRING(30),
    allowNull:false
},
zipCode:{
    type:Sequelize.INTEGER,
    allowNull:false
},
classification:{
    type:Sequelize.CHAR,
    allowNull:false
},
warehouseId:{
    type:Sequelize.INTEGER,
    allowNull:true
},
coi:{
    type:Sequelize.STRING(20),
    allowNull:false
},
paymentTerms:{
    type:Sequelize.TEXT,
    allowNull:true
},
creditLimit:{
    type:Sequelize.DATE,
    allowNull:false
},
active:{
     type:Sequelize.BLOB,
     allowNull:false,
     defaultValue: false
}